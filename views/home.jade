extends layout

block content
  h1 Welcome #{user.name}
  #feed
    for post in posts
      .post(id=post._id)
        h4 
          a(href='/course/'+post.course._id) #{post.course.name}
        p= post.message
        h6= post.from.name

  script
    $(document).ready(function() {
        pollFeed();
    });